<% for(var i =0; i < books.length;i++){ %>
<tr>
    <td><%= i+1 %></td>
    <td><%= books[i].bookName %></td>
    <td><%= books[i].author %></td>
    <td><%= books[i].status %></td>
    <td>
        <a href="/update-product" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-edit"></i></span>
        </a>
        <a href="#" class="btn border-shadow delete">
            <span class="text-gradient"><i class="fas fa-trash-alt"></i></span>
        </a>
    </td>
</tr>

<% } %>